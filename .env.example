# Environment Variables
# Copy this file to .env and fill in your values

# Backend API URLs
# For local development
VITE_API_URL=http://localhost:5000/api
# For OAuth server (might be different in production)
VITE_AUTH_SERVER_URL=http://localhost:5000/api

# Frontend URLs
# Used for redirects after authentication
VITE_FRONTEND_URL=http://localhost:5173
# Used for success page redirect
VITE_DASHBOARD_URL=http://localhost:5173/dashboard

# Discord OAuth Configuration
VITE_DISCORD_CLIENT_ID=your_discord_client_id_here
# This should match what's configured in Discord Developer Portal
VITE_DISCORD_REDIRECT_URI=${VITE_AUTH_SERVER_URL}/auth/discord/callback 